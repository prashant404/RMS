<style>
  .profileBox {
    text-align: center;
    margin-top: 30px;
    padding: 20px;
    border: 1px solid #ccc;
    border-radius: 5px;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
  }

  .profileBox h1 {
    margin-bottom: 10px;
    font-size: 24px;
  }

  .profileBox h2 {
    font-size: 18px;
  }

  .stuTable {
    margin-top: 30px;
  }

  .table {
    width: 100%;
    border-collapse: collapse;
    border: 1px solid #ccc;
  }

  .table th, .table td {
    padding: 10px;
    text-align: center;
    border: 1px solid #ccc;
  }

  .table th {
    background-color: #f8f9fa;
  }

  .table tr:nth-child(even) {
    background-color: #f2f2f2;
  }
</style>

<div class="profileBox">
  <h1>Name: <%= one.name %></h1>
  <h2>Roll No.: <%= one.roll %></h2>
</div>

<div class="stuTable">
  <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Subject Name</th>
        <th scope="col">Score</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <th scope="row">1</th>
        <td>English</td>
        <td><%= one.english %></td>
      </tr>
      <tr>
        <th scope="row">2</th>
        <td>Maths</td>
        <td><%= one.math %></td>
      </tr>
      <tr>
        <th scope="row">3</th>
        <td>Science</td>
        <td><%= one.science %></td>
      </tr>
    </tbody>
  </table>
</div>
